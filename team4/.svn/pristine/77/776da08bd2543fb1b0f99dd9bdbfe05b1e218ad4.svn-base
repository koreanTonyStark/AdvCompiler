//
// test01
//
// simple computations with integers
//
// expected output: 710-3 (no newline)
//

module test01_special_case;

var a : integer;
    b : integer;
    c : integer;

function Tmp(n: integer):integer;
begin
  if (n>0) then
    return 10
  else
    return 10
  end
end Tmp;

begin
  a := 2;
  b := 5;

  c := a+b;
  WriteInt(c);

  c := a*b;
  WriteInt(c);

  c := -b+a;
  WriteInt(c);
  c := Tmp(1);
  WriteInt(Tmp(1))
end test01_special_case.
